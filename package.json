{
  "name": "villain-react",
  "author": "Baltazar Gomez",
  "version": "2.0.0",
  "license": "MIT",
  "homepage": "https://github.com/btzr-io/Villain",
  "description": "An open source web based comic book reader as a react component",
  "files": [
    "dist/"
  ],
  "keywords": [
    "comic book",
    "react",
    "reader",
    "viewer"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "format": "yarn pretty-quick",
    "start": "nollup -c --environment NODE_ENV:development",
    "build": "npm run clean && rollup -c --environment NODE_ENV:production"
  },
  "dependencies": {
    "@mdi/js": "^7.0.96",
    "@mdi/react": "^1.6.1",
    "clsx": "^1.2.1",
    "libarchive.js": "^1.3.0",
    "openseadragon": "^3.1.0",
    "react-compound-slider": "^3.4.0"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "nollup": "^0.18.4",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-refresh": "^0.10.0",
    "rollup": "^2.56.3",
    "rollup-plugin-commonjs-alternate": "^0.8.0",
    "rollup-plugin-hot-css": "^0.5.1",
    "rollup-plugin-react-refresh": "0.0.3",
